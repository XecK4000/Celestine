{

	"manifest_version": 2,
	"name": "Célestine",
	"version": "1.0",

	"description": "Une chouette pour vous accompagner dans toutes vos activités",

	"icons": {
		"48": "icons/celestine48.png",
		"96": "icons/celestine96.png"
	},

	"background": {
		"scripts": [
			"jquery-3.2.1.min.js",
			"jquery-ui.min.js",
			"celestine.js"
		]
	},

	"options_ui": {
		"page": "options.html"
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["jquery-3.2.1.min.js"]
		},
		{
			"matches": ["<all_urls>"],
			"js": ["jquery-ui.min.js"]
		},
		{
			"matches": ["<all_urls>"],
			"js": ["celestine.js"]
		}
	],

	"permissions": [
		"tabs",
		"storage"
	],

